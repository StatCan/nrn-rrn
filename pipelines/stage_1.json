{
  "pipeline": {
    "name": "stage_1"
  },
  "description": "A pipeline that executes NRN stage 1.",
  "transform": {
    "cmd": ["python3", "stage_1.py"],
    "image": "artifactory.cloud.statcan.ca/docker-local/daaas/nrn",
    "env": {
      "nrn_working_dir": "src/stage_1"
    }
  },
  "input": {
    "pfs": {
      "name": "data",
      "repo": "raw",
      "glob": "/"
    }
  }
}