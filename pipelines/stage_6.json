{
  "pipeline": {
    "name": "stage_6"
  },
  "standby" : false,
  "description": "A pipeline that executes NRN stage 6.",
  "transform": {
    "image": "artifactory.cloud.statcan.ca/docker-local/daaas/nrn:2020.03.11-2",
    "image_pull_secrets": [ "artifactory-prod" ],
    "cmd" : [ "bash" ],
    "stdin" : [ "rm -rf /pfs/data/raw/vintage",
		"rm -rf /nrn-app/data/raw/vintage",
		"rm /pfs/data/raw/vintage.zip",
		"rm /nrn-app/data/raw/vintage.zip",
		"rm /nrn-app/data/raw/vintage.zip",
		"chmod u+x /nrn-app/pipelines/run.sh",
		"/nrn-app/pipelines/run.sh"
	      ],
    "env": {
      "nrn_working_dir": "/nrn-app/src/stage_6"
    }
  },
  "input": {
    "pfs": {
      "repo" : "stage_5",
      "name": "data",
      "glob": "/"
    }
  }
}
